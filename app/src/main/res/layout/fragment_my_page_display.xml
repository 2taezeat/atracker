<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".ui.MyPageDisplayFragment">



        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/myPageHeaderCL"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@color/atracker_header_transparent"
            >

<!--            <ImageView-->
<!--                android:layout_width="24dp"-->
<!--                android:layout_height="16dp"-->
<!--                app:layout_constraintTop_toTopOf="parent"-->
<!--                app:layout_constraintStart_toStartOf="parent"-->
<!--                android:src="@drawable/ic_back_icon_raw"-->
<!--                android:id="@+id/myPageBackButton"-->
<!--                android:layout_marginStart="20dp"-->
<!--                app:layout_constraintBottom_toBottomOf="parent"-->

<!--                >-->
<!--            </ImageView>-->

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/myPageHeaderTitle"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:text="@string/my_page_title"
                android:textSize="16sp"
                android:textColor="@color/atracker_white"
                app:layout_constraintBottom_toBottomOf="parent"
                />


        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@+id/myPageHeaderCL"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/myPageMainCL"

            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/myPageNickNameTV"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:text="제시카"
                android:layout_marginStart="28dp"
                android:textColor="@color/progress_color_7"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginTop="30dp"

                >


            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/my_page_hello_title"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@+id/myPageNickNameTV"
                android:textColor="@color/atracker_white"
                android:textSize="24sp"
                android:layout_marginTop="30dp"

                >
            </TextView>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/position_title"
                app:layout_constraintTop_toBottomOf="@+id/myPageNickNameTV"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="32dp"
                android:textColor="@color/atracker_gray_3"
                android:textSize="14sp"
                android:layout_marginTop="26dp"
                >

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPageNickNameTV"
                app:layout_constraintStart_toEndOf="@+id/myPageNickNameTV"
                android:id="@+id/myPagePositionTV"
                tools:text="애널리스트"
                android:layout_marginStart="4dp"
                android:textColor="@color/atracker_white"
                android:textSize="16sp"
                android:layout_marginTop="26dp"

                >

            </TextView>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/career_age_title"
                app:layout_constraintTop_toBottomOf="@+id/myPagePositionTV"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="32dp"
                android:textColor="@color/atracker_gray_3"
                android:textSize="14sp"
                android:layout_marginTop="12dp"
                >

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPagePositionTV"
                app:layout_constraintStart_toEndOf="@+id/myPageNickNameTV"
                android:id="@+id/myPageCareerAgeTV"
                tools:text="신입"
                android:layout_marginStart="4dp"
                android:textColor="@color/atracker_white"
                android:textSize="16sp"
                android:layout_marginTop="12dp"

                >

            </TextView>



            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/myPageCareerAgeTV"
                android:background="@color/atracker_gray_6"
                android:id="@+id/myPageView1"
                android:layout_marginTop="32dp"
                >

            </View>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPageView1"
                android:text="@string/account_setting_title"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/myPageAccountSettingTV"
                android:layout_marginTop="42dp"
                android:layout_marginStart="28dp"
                android:textColor="@color/atracker_white"
                android:textSize="16sp"
                >

            </TextView>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/myPageAccountSettingTV"
                android:background="@color/atracker_gray_6"
                android:id="@+id/myPageView2"
                android:layout_marginTop="13dp"
                >

            </View>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPageView2"
                android:text="@string/logout"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/myPageLogoutTV"
                android:layout_marginStart="28dp"
                android:layout_marginTop="13dp"
                android:textColor="@color/atracker_gray_3"
                android:textSize="14sp"

                >

            </TextView>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/myPageLogoutTV"
                android:background="@color/background_light_gray"
                android:id="@+id/myPageView3"
                android:layout_marginTop="13dp"
                >

            </View>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPageView3"
                android:text="@string/account_withdrawal_title"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/myPageAccountWithdrawalTV"
                android:layout_marginStart="28dp"
                android:layout_marginTop="13dp"
                android:textColor="@color/atracker_gray_3"
                android:textSize="14sp"

                >

            </TextView>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/myPageAccountWithdrawalTV"
                android:background="@color/background_light_gray"
                android:id="@+id/myPageView4"
                android:layout_marginTop="13dp"

                >
            </View>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPageView4"
                android:text="@string/help_title"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/myPageHelpTV"
                android:layout_marginStart="28dp"
                android:layout_marginTop="32dp"
                android:textColor="@color/atracker_white"
                android:textSize="16sp"
                >

            </TextView>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/myPageHelpTV"
                android:background="@color/atracker_gray_6"
                android:id="@+id/myPageView5"
                android:layout_marginTop="13dp"
                >

            </View>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPageView5"
                android:text="@string/contact_title"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/myPageContactTV"
                android:layout_marginStart="28dp"
                android:layout_marginTop="13dp"
                android:textColor="@color/atracker_gray_3"
                android:textSize="14sp"
                >

            </TextView>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/myPageView5"
                app:layout_constraintStart_toEndOf="@+id/myPageContactTV"
                android:layout_marginStart="40dp"
                android:layout_marginTop="13dp"
                android:textColor="@color/atracker_gray_6"
                android:textSize="14sp"
                android:text="@string/brianrudder_email"
                >

            </TextView>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintTop_toBottomOf="@+id/myPageContactTV"
                android:background="@color/background_light_gray"
                android:id="@+id/myPageView6"
                android:layout_marginTop="13dp"
                >
            </View>


            <ImageView
                android:layout_width="80dp"
                android:layout_height="24dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginBottom="24dp"
                android:src="@drawable/ic_login_atracker_image"
                >


            </ImageView>







        </androidx.constraintlayout.widget.ConstraintLayout>




